{
	"name": "baraja-core/php-moon-phase",
	"description": "A PHP class for calculating the phase of the Moon.",
	"homepage": "https://github.com/baraja-core/php-moon-phase",
	"keywords": [
		"php",
		"moon",
		"phase"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Barášek",
			"email": "jan@barasek.com"
		},
		{
			"name": "Samir Shah",
			"email": "samir@rayofsolaris.net"
		},
		{
			"name": "Erik Scheepers",
			"email": "e.scheepers@linkorb.com",
			"role": "Packaging for composer"
		}
	],
	"require": {
		"php": ">=7.1.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6",
		"symplify/easy-coding-standard": "^7.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
